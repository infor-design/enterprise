{
  "private": true,
  "scripts": {
    "start": "npx grunt clean:app && npx grunt sass:app && node app/server.js",
    "stop": "./scripts/stop.sh",
    "eslint": "eslint -c .eslintrc.js src",
    "eslint:log": "eslint -c .eslintrc.js src -o eslint.log",
    "eslint:error-only": "eslint -c .eslintrc.js --quiet src",
    "build": "npx rollup -c",
    "minify": "node ./scripts/minify.js",
    "functional:local": "npx karma start test/karma.conf.js",
    "functional:ci": "npx karma start test/karma.conf.js --single-run",
    "e2e:local:pre": "npm run webdriver:update && (npm run start & sleep 10)",
    "e2e:local": "npm run e2e:local:pre && (protractor test/protractor.conf.js || npm run stop) && npm run e2e:local:post",
    "e2e:local:post": "npm run stop && exit 0",
    "e2e:ci:pre": "npm run webdriver && (npm run start & sleep 10)",
    "e2e:ci": "npm run e2e:ci:pre && protractor test/protractor.ci.conf.js || npm run stop",
    "webdriver": "webdriver-manager update --standalone false --gecko false --quiet",
    "webdriver:update": "webdriver-manager update",
    "prepublishOnly": "npx grunt publish publish",
    "publish-dev": "npx grunt publish && npm publish publish/ --tag dev",
    "publish-nightly": "npx grunt publish && npm publish publish/ --tag nightly",
    "publish-prod": "npx grunt publish && npm publish publish/",
    "documentation": "node ./scripts/deploy-documentation.js",
    "publish-aws": "AWS_PROFILE=sohoxi directory-to-s3 -d publish infor-devops-core-soho-us-east-1/sohoxi/4.4.0 -v"
  },
  "dependencies": {
    "d3": "^4.13.0",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "axe-core": "^3.0.2",
    "axe-webdriverjs": "^2.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "browserstack-local": "^1.3.1",
    "chai-jquery": "^2.0.0",
    "chalk": "^2.3.2",
    "connect-multiparty": "^2.0.0",
    "del": "^3.0.0",
    "directory-to-s3": "^1.0.0",
    "documentation": "^5.3.5",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jasmine": "^2.9.3",
    "eslint-plugin-jasmine-jquery": "^1.0.0",
    "express": "^4.13.4",
    "express-csp": "^0.1.3",
    "express-slash": "^2.0.1",
    "extend": "^3.0.0",
    "front-matter": "^2.3.0",
    "grunt": "^1.0.2",
    "grunt-banner": "^0.6.0",
